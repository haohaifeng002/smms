<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.gradDesign.smms.domain.SuppInfo" table="supp_info">
    	<comment>supp_info</comment>
        <!-- single primaryKey -->
	    <id name="supId" type="string" length="32">
	    	<column name="SUP_ID">
	    	</column>
            <generator class="uuid.hex"/>
        </id>     
		
	    <property name="supNum" type="string"  length="50"  not-null="false">
	    	<column name="SUP_NUM">
	    	</column>
    	</property>  
         
	    <property name="supName" type="string"  length="100"  not-null="false">
	    	<column name="SUP_NAME">
	    	</column>
    	</property>  
         
	    <property name="supPost" type="string"  length="6"  not-null="false">
	    	<column name="SUP_POST">
	    	</column>
    	</property>  
         
	    <property name="supAddr" type="string"  length="200"  not-null="false">
	    	<column name="SUP_ADDR">
	    	</column>
    	</property>  
         
	    <property name="linkman" type="string"  length="100"  not-null="false">
	    	<column name="LINKMAN">
	    	</column>
    	</property>  
         
	    <property name="phone" type="string"  length="32"  not-null="false">
	    	<column name="PHONE">
	    	</column>
    	</property>  
         
	    <property name="remark" type="string"  length="500"  not-null="false">
	    	<column name="REMARK">
	    	</column>
    	</property>  
         
    </class>
</hibernate-mapping>